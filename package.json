{
  "name": "wki-wip",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host",
    "dev:server": "nodemon --watch server --exec tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist-server/index.js",
    "preview": "vite preview",
    "preview:network": "vite preview --host",
    "preview:all": "concurrently \"npm run start\" \"npm run preview:network\"",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "lucide-react": "^0.545.0",
    "mongoose": "^8.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "~5.9.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
